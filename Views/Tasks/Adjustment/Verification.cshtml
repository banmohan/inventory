@using MixERP.Inventory.Extensions
@model dynamic

@{
    ViewBag.Title = I18N.InventoryTransfers;
    Layout = ViewBag.InventoryLayoutPath;
}

@Html.FinancePartialView("Shared/SearchView.cshtml", TenantConvention.GetTenant())
<script src="/Areas/MixERP.Inventory/scripts/tasks/transfer/index.js"></script>

<script>
    var annotationStatus = window.Enumerable.From(window.model.Annotation).Where(function(x) {
        return x.Id === "Status";
    }).FirstOrDefault();


    annotationStatus.DefaultValue = "Unverified";
    window.model.HasVerification = true;
    window.model.Title = window.translate("InventoryAdjustmentsVerification");

    prepareView(model);
</script>

